{
  "name": "@dominikzuchowicz/firstep",
  "version": "2.0.0",
  "type": "module",
  "packageManager": "pnpm@10.0.0",
  "scripts": {
    "dev": "tsx watch src/devServer.ts",
    "start": "tsx src/devServer.ts",
    "type-check": "tsc --noEmit",
    "clean": "rimraf dist",
    "build": "pnpm clean && tsc -p tsconfig.prod.json && tsc-alias -p tsconfig.prod.json",
    "prepublishOnly": "npm run build",
    "lint": "eslint --fix",
    "lint:prettier": "prettier --check . --ignore-path .gitignore",
    "format": "prettier --write ./"
  },
  "author": "kidi",
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.9.0",
    "@types/eslint": "^9.6.0",
    "@types/eslint__js": "^8.42.3",
    "@types/express": "^4.17.21",
    "eslint": "^8.57.0",
    "globals": "^15.9.0",
    "prettier-plugin-organize-imports": "^4.0.0",
    "rimraf": "^6.0.1",
    "tsc-alias": "^1.8.10",
    "tsx": "^4.16.2",
    "typescript": "^5.5.4",
    "typescript-eslint": "^7.18.0",
    "vite": "^5.4.2"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "citty": "^0.1.6",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "http-status-codes": "^2.3.0",
    "morgan": "^1.10.0"
  }
}
